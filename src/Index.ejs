    </div>

    <div class="main-body">
        <img id = "front-logo" src="/images/_51248235-4728-4a41-a966-d383c13ce461.png" alt="">
        <div class="All-row">
            <% if (sectionProducts[0].products.length > 0) { %>
                <% const product = sectionProducts[0].products[0]; %>
                <div class="boxes" id="bx1">
                    <h1><%= product.Name %></h1>
                    <img id="backdrop" src="data:image/png;base64,<%=product.product_img%>" alt="<%= product.Name %>" width="270" height="412">
                    <p class="price">Price: <%= product.Price %></p>
                    <p class="description"><%= product.short_Description %></p>
                    <button id="fav" onclick="add_favourite(<%=product.ProductID%>,empty00,full00)"><i class="fa-solid fa-star fa-lg" id = "full00" style="display: none;"></i><i class="fa-regular fa-star fa-lg" id = "empty00" style="display: block;"></i></button>

                    <div class="buttons">
                        <button class="add-to-cart" onclick="movecart(<%=product.ProductID%>);">Add to Cart</button>
